/* pages/home/home.wxss */

.container {
  width: 100%;
  height: 100%;
  background-color: red,
} 

.swipers{
  padding-bottom: 10px;
}

swiper-item image{
  width: 100%;
}

/* 图标导航 */
.navs{
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  background-color: #fff;
  margin-top: 10px;
}

.nav-item{
  width: 25%;
  padding: 10px;
  display: inline-block;
  text-align: center;
  box-sizing: border-box;
}

.iconPic{
  width: 88rpx;
  height: 88rpx;
}

.iconTitle{
  color: #666;
  font-size: 14px;
}

/* 文章页 */
.article{
  padding: 10px;
  background-color: #fff;
  margin-top: 10px;
}

.article-item{
  padding: 10px;
  margin: 10px;
}

.articlePic{
  width: 100%;
  height: 260rpx;
}

.articleTitle{
  color: #666;
  font-size: 14px;
}
